  },
  "scripts": {
    "start": "expo start",
    "startForTests": "expo start --ios",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint .",
    "prepush": "npm run lint && npm test",
    "publish-production": "npx expo-optimize ./ && expo publish --release-channel production",
    "publish-staging": "npx expo-optimize ./ && expo publish",
    "build-android-production": "npx expo-optimize ./ && expo build:android -t app-bundle --release-channel production",
    "build-ios-production": "npx expo-optimize ./ && expo build:ios -t app-bundle --release-channel production",
    "upload-ios": "expo upload:ios",
    "detoxtest": "detox test",
    "e2e": "npm-run-all -p -r startForTests detoxtest",
    "test:coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11"
      }
    }
  }
}
