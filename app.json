{
  "expo": {
    "name": "react native expo starter",
    "description": "Access, review and manage your benefits account with this mobile application.",
    "slug": "expo-starter",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon-white-drop.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.placeholder.app",
      "versionCode": 10,
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.placeholder.app",
      "supportsTablet": true,
      "usesIcloudStorage": true
    },
    "notification": {
      "icon": "./src/assets/notification-icon.png"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "placeholder___",
            "project": "placeholder-mobile",
            "authToken": ""
          }
        }
      ]
    }
  }
}
